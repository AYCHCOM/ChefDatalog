  <% @instances.each do |i| -%>
  - kafka_connect_str: <%= i['kafka_connect_str'] %>
    zk_connect_str: <%= i['zk_connect_str'] %>
    <% if i['consumer_groups'] -%>
    consumer_groups: <%= i['name'] %>
      <% i["consumer_groups"].each do |consumer, t| -%>
        <%= consumer %>:
          <% t.each do |topic, l| -%>
            <%= topic %>: <%= l %>
          <% end -%>
        <% end -%>
      <% end -%>
    <% end -%>
  <% end -%>

init_config:
